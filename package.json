{
  "name": "github-nerd",
  "version": "1.0.0",
  "description": "react web page for viewing user's github activity",
  "homepage": "https://nntrn.github.io/github-nerd/",
  "author": "annie tran (https://github.com/nntrn)",
  "private": true,
  "keywords": [
    "github",
    "api",
    "react"
  ],
  "scripts": {
    "clear-babel-cache": "rm -rf ./node_modules/.cache/babel-loader/*",
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "css": "stylelint src/style/*.scss --fix",
    "lint": "eslint src/ --fix"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.37.5"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
